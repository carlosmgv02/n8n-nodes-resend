{
  "name": "n8n-nodes-api-resend",
  "version": "1.0.1",
  "description": "n8n node for Resend - Email marketing and transactional emails",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "resend",
    "email",
    "marketing",
    "broadcasts",
    "transactional",
    "webhook",
    "trigger",
    "automation"
  ],
  "license": "MIT",
  "homepage": "https://github.com/carlosmgv02/n8n-nodes-api-resend",
  "author": {
    "name": "Carlos Martínez García-Villarrubia",
    "email": "carlosmgv02@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carlosmgv02/n8n-nodes-api-resend.git"
  },
  "bugs": {
    "url": "https://github.com/carlosmgv02/n8n-nodes-api-resend/issues"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint 'nodes/**/*.ts' 'credentials/**/*.ts' --ext .ts",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ResendApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Resend/Resend.node.js",
      "dist/nodes/Resend/ResendTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-n8n-nodes-base": "^1.0.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.0.0",
    "typescript": "~5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
